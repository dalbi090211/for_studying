#include <iostream>
#include <string>
using namespace std;
 
class member {
private :
    int id;
    string name;
    int phoneNumber;
    char gender;
    
public :
    // 내부 클래스 정의
    void print_name() {
        cout << name;
    }
 
    // 외부 클래스 정의
    void print_id();
};
 
// 외부 클래스 정의
void member :: print_id() {
    cout << id;
}
 
int main(void) {
    member m1;
    
    return 0;
}